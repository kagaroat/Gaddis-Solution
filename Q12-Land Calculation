// Program that converts land of square feet in acre

#include <iostream>                     //Including file for Input/Output
using namespace std;                    //Converting the Input/Output into standard form

//Main function of program
int main ()
{
    double one_acre= 43560;             //number of square feet in one acre
    double land = 389767;               //number of square feet in land

    //formula for converting land's square feet in acres
    double acres = land / one_acre;

    //printing the content
	cout << "The number of acres in 389767 square feet of land is = " << acres << " acres" << endl;
	return 0;
}
