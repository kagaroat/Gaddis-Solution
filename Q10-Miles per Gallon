//Calculates the number of miles per gallon the car gets

#include <iostream>                             //Including file for Input/Output
using namespace std;                            //Converting the Input/Output into standard form

//Main function of program
int main()
{
    double miles = 350.0;
	  double gallons = 12.0;

    double mpg = miles / gallons;               //miles per gallon (MPG)

    //printing the content
	  cout << "This car gets " << mpg << " Miles Per Gallon " << endl;

	  return 0;
}
